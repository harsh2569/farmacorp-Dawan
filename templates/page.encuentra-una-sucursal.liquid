    width: 16px;
    height: 16px;
    margin-top: 0px;
    margin-bottom: 0px;
}
ul.store-filters li span {
    font-family: 'Inter';
    font-style: normal;
    font-weight: 500;
    font-size: 14px;
    line-height: 20px;
}

.footer_filter {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

